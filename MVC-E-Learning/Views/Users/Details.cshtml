@model UserViewModel
@{
    ViewData["Title"] = "Details";
}

<div class="row">
	<div class="agileits-top">
			
			<div class="mb-3">
				<label for="FirstName" class="form-label">First Name</label>
				<input type="text" asp-for="FirstName" class="form-control" id="FirstName" readonly>
			</div>

			<div class="mb-3">
				<label for="LastName" class="form-label">Last Name</label>
				<input type="text" asp-for="LastName" class="form-control" id="LastName" readonly>
			</div>

			<div class="mb-3">
				<label for="Email" class="form-label">Email address</label>
				<input type="email" asp-for="Email" class="form-control" id="Email" readonly>
			</div>

			<div>
				<label for="Discriminator" class="form-label">User Type</label>
				<input type="email" asp-for="Discriminator" class="form-control" id="Discriminator" readonly>

			</div><br />
	</div>
</div>

<div class="mt-3">
    <a asp-action="Edit" class="btn btn-info" asp-route-id="@Model.Id">Edit</a>
    <a asp-action="Index" class="btn btn-warning">Back To List</a>
</div>
